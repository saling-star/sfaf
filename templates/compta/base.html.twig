{% extends 'base_b.html.twig' %}

{% block title %}{{ llll.title|trans }}{% endblock %}

{% block navigation %}{% endblock %}

{% block main %}
<div class=row>
    <div class="col-sm-1"></div>
    <div class="col-sm-10">
        <div class=row>
            <div class=col>
            {% block charges %}{% endblock %}
            </div>
            <div class=col>
            {% block produits %}{% endblock %}
            {% block comptexp %}{% endblock %}
            </div>
        </div>

        <div class=row>
            <div class=col>
            {% block actif %}{% endblock %}
            </div>
            <div class=col>
            {% block passif %}{% endblock %}
            {% block bilan %}{% endblock %}
            </div>
        </div>

        <div class=row>
            <div class=col>
            {% block amortis %}{% endblock %}
            </div>
        </div>
    </div>
    <div class="col-sm-1"></div>
</div>
{% endblock %}

{% block footer %}{% endblock %}
