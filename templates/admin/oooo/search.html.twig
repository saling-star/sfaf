{% extends 'admin/oooo/base_side.html.twig' %}

{% block title %}{{ llll.title|trans }}{% endblock %}

{% block main %}
    <form action="search" method="get">
        <div class="form-group">
            <input name="q"
                   class="form-control search-field"
                   placeholder="{{ 'doc.search_for'|trans }}"
                   autocomplete="off"
                   value="{{ query }}"
                   autofocus
                   data-no-results-message="{{ 'doc.search_no_results'|trans }}"
            >
        </div>
    </form>

    <div id="results">
    {% set tbsort = 'dcmnts' %}
    {% include "_partials/_dcmnts_list.html.twig" %}
    </div>
{% endblock %}
